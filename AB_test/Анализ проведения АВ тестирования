  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "participants = pd.read_csv(\"/datasets/final_ab_participants.csv\")"
   ]
  },
  {
   "cell_type": "code",
