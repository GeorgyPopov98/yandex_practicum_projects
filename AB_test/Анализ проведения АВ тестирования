    "events_test_b = events_test[events_test['group']=='B'].groupby('event_name'\n",
    "        ).agg({'user_id': ['nunique'\n",
    "              ]}).reset_index(level='event_name')\n",
    "events_test_b.columns = ['event_name', 'unique']\n",
